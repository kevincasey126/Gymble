<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #00bfff;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProfileSettingsScene">
   <children>
      <VBox prefHeight="400.0" prefWidth="600.0">
         <children>
            <HBox prefHeight="44.0" prefWidth="600.0">
               <children>
                  <Button fx:id="back_button" mnemonicParsing="false" onAction="#pressedBackButton" text="Back" />
                  <Label fx:id="title_label" alignment="CENTER" prefHeight="53.0" prefWidth="519.0" text="Profile Settings">
                     <font>
                        <Font size="36.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="400.0" prefWidth="600.0">
               <children>
                  <VBox alignment="CENTER" prefHeight="350.0" prefWidth="300.0" spacing="15.0">
                     <children>
                        <Pane prefHeight="46.0" prefWidth="300.0" />
                        <Button fx:id="update_bio_button" mnemonicParsing="false" onAction="#pressedUpdateBioButton" prefHeight="100.0" prefWidth="200.0" text="Update Bio" />
                        <Button fx:id="update_pic_button" mnemonicParsing="false" onAction="#pressedUpdateProfilePicButton" prefHeight="100.0" prefWidth="200.0" text="Update Profile Picture" />
                        <Button fx:id="update_gym_button" mnemonicParsing="false" onAction="#pressedUpdateGymButton" prefHeight="100.0" prefWidth="200.0" text="Update Home Gym" />
                        <Button mnemonicParsing="false" onAction="#pressedUpdateContactInfoButton" prefHeight="100.0" prefWidth="200.0" text="Update Contact Info" />
                        <Pane prefHeight="44.0" prefWidth="300.0" />
                     </children>
                  </VBox>
                  <BorderPane prefHeight="200.0" prefWidth="300.0">
                     <top>
                        <Pane prefHeight="25.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
                     </top>
                     <right>
                        <Pane prefHeight="200.0" prefWidth="25.0" BorderPane.alignment="CENTER" />
                     </right>
                     <bottom>
                        <Pane prefHeight="25.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
                     </bottom>
                     <center>
                        <ScrollPane prefHeight="0.0" prefWidth="275.0" BorderPane.alignment="CENTER">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="450.0" prefWidth="260.0">
                                 <children>
                                    <VBox prefHeight="495.0" prefWidth="260.0">
                                       <children>
                                          <ImageView fx:id="profile_pic" fitHeight="250.0" fitWidth="260.0" pickOnBounds="true" />
                                          <TextField fx:id="name_field" prefHeight="45.0" prefWidth="260.0">
                                             <font>
                                                <Font size="24.0" />
                                             </font></TextField>
                                          <TextField fx:id="contact_info" prefHeight="45.0" />
                                          <TextArea fx:id="bio_area" prefHeight="150.0" prefWidth="260.0" />
                                       </children></VBox>
                                 </children></AnchorPane>
                          </content>
                        </ScrollPane>
                     </center></BorderPane>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
